var twoSum = function(nums, target) {
let start= 0,end = 1

    while(start<end ){
        if(nums[start] + nums[end] === target ){
            return [start,end]
        }else{
           if(end< nums.length-1){
               end ++ 
           }else{
               start ++
               end = start + 1
           }
        }
    }

};
