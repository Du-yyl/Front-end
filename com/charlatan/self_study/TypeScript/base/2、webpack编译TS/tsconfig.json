{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "removeComments": true,
    "noEmitOnError": false,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noFallthroughCasesInSwitch": false,
    "sourceMap": false
  },
  "exclude": [
    "node_modules"
  ]
}
